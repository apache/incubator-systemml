/*
 * DML script to test Singular Value Decomposition (SVD)
 */
 
 A = read($1);

[U, S, V] = svd(A);

# U and V must be orthogonal i.e., U = U^{-1} and V = V^{-1}
I = U %*% t(U);
I = V %*% t(V);

D = 0.0;

write (I, $2);
write (D, $3);
