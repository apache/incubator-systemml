test_num = $in_test_num;
row_part = $in_rp;

if(row_part) {
  X = rbind(read($in_X1), read($in_X2));
}
else {
  X = cbind(read($in_X1), read($in_X2));
}

if(test_num == 1) {
  # X= matrix(seq(1,4000000), rows=2000, cols=2000);
  Y= matrix(2, rows=2000, cols=1);

  lamda = sum(Y);
  Z = round(abs(X + lamda)) + 5;
}
else if(test_num == 2) {
  # X = matrix( 1, rows=10, cols=10);
  Z = 1/(1+exp(-X));
}
else if(test_num == 3) {
  # X = matrix("1 2 3 4 5 6 7 8 9", rows=3, cols=3);
  Z = 10 + floor(round(abs(7 + (1 / exp(X)))));
}
else if(test_num == 4) {
  # X = matrix("1 2 3 4 5 6 7 8 9", rows=3, cols=3);
  w = matrix(3, rows=4, cols=4);
  v = matrix(5, rows=4, cols=4);
  Z = 10 + floor(round(abs((X + w) * v)));
}
else if(test_num == 5) {
  # X = matrix("1 2 3 4 5 6 7 8 9", rows=3, cols=3);
  w = matrix("1 2 3 4", rows=4, cols=1);
  v = matrix("4 4 4 4", rows=4, cols=1);

  G = abs(exp(X));
  Y = 10 + floor(round(abs((X / w) + v)));
  Z = G + Y;
}
else if(test_num == 6) {
  # X = matrix("1 2 3", rows=3, cols=1);
  y = matrix("1 1 1 1", rows=4, cols=1);
  v = matrix("3 3 3 3", rows=4, cols=1);

  Z = as.matrix(sum(X * y * v));
}
else if(test_num == 7) {
  # X = seq(7, 1007);
  Y = seq(6, 1006);

  U = X + Y - 7 + abs(X);
  Z = t(U) %*% U;
}
else if(test_num == 8) {
  # X = matrix(1, rows=1002, cols=23);
  Y = seq(1, 1002);
  X[100:900,] = matrix(0, rows=801, cols=23);

  Z = X * ((X + 7.7) * Y);
}
else if(test_num == 9) {
  # X = matrix(seq(7, 1006), 500, 2);
  Y = matrix(seq(6, 1005), 500, 2);

  U = X + 7 * Y;
  Z = as.matrix(sum(U^2))
}

write(Z, $out_Z);
