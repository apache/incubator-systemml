xorTest = function() {

  tmp11 = xor(1, 1);
  print("xor of 1, 1 is "+tmp11);

  tmp10 = xor(1, 0);
  print("xor of 1, 0 is "+tmp10);

  tmp01 = xor(0, 1);
  print("xor of 0, 1 is "+tmp01);

  tmp00 = xor(0, 0);
  print("xor of 0, 0 is "+tmp00);

}

tmp = xorTest();