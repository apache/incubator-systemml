sum = function(matrix[double] X) return (double total)
{
    total = 0
    for (i in 1:nrow(X))
    {
        for (j in 1:ncol(X))
        {
            total = total + as.scalar(X[i,j])
        }
    }
    print("Override sum is " + total)
}

min = function(matrix[double] X) return (double minimum)
{
    minimum = min(X)
    print("Minimum is " + minimum)
}
