printMatrix = function(Matrix[Double] m, string name) return() {
    print("--------------------------------------")
    print(":: [MATRIX] " + name + "  (col:" + ncol(m) + ",row:" + nrow(m) + ")")
    for(i in 1:nrow(m)) {
        print(":(" + i + ")   " + toString(m[i,], linesep=" ", sep="    "))
    }
    print("--------------------------------------")
}