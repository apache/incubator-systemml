def sum(X: matrix[float]) -> (total: float):
    total = 0
    for (i in 1:nrow(X)):
        for (j in 1:ncol(X)):
            total = total + scalar(X[i,j])
    print("Override sum is " + total)

def min(X: matrix[float]) -> (minimum: float):
    minimum = min(X)
    print("Minimum is " + minimum)
